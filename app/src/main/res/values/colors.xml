    <!--Color palette-->

    <color name="colorPrimary">#A5D6A7</color> <!--Color primary (dark)-->
    <color name="colorPrimaryLight">#66BB6A</color> <!--Color primary (light)-->
    <color name="colorPrimaryVariant">#81C784</color> <!--Color primary variant (dark)-->
    <color name="colorPrimaryVariantDark">#4CAF50</color> <!--Color primary variant (light)-->

    <color name="colorSecondary">#FFCA28</color> <!--Color secondary-->
    <color name="colorSecondaryVariant">#FFC107</color> <!--Color secondary variant-->

    <color name="red_200">#cf6679</color> <!--Color error (dark)-->
    <color name="red_600">#b00020</color> <!--Color error (light)-->

    <!--Color surface (light), color on error (light), -->
    <!--color on surface (dark), color on surface (dark)-->
    <color name="white_50">#ffffff</color>

    <color name="black_800">#212121</color> <!--Color surface (dark)-->
    <!--Color on secondary (light), color on background (light),-->
